{
  "presets": [
    "es2015",
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            ">0.25%"
          ]
        }
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        "require-context-hook"
      ]
    }
  }
}
